{% extends 'basefront.html.twig' %}

{% block body %}


<h3>   les appel offres     </h3>


	<div class="row g-4">
		{% for offre in b %}
			<div class="col-lg-12 col-md-6 wow zoomIn" data-wow-delay="0.6s" style="visibility: visible; animation-delay: 0.6s; animation-name: zoomIn;">
				<div class="service-item d-flex flex-column justify-content-center text-center rounded">
					<div class="service-icon flex-shrink-0">
			<i class="fa fa-home fa-2x"></i>

					</div>
					<h5 class="mb-3">{{ offre.nom}}
					</h5>
					<p>
						nom :
						{{  offre.quantite}}</p>
					<p>
						quantite :
						{{  offre.budget }}</p>
					
					<p>
						budget :
						{{  offre.description }}</p>
					
				</div>
			</div>
		{% endfor %}




{% endblock %}

