<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\AppelD'offre" table="appel_d'offre">
    <indexes>
      <index name="fk1" columns="id_utilisateur"/>
      <index name="fk10" columns="id_categorie"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="quantité" type="integer" column="quantité" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="budget" type="float" column="budget" precision="10" scale="0" nullable="false"/>
    <field name="description" type="string" column="description" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="date" type="date" column="date" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <many-to-one field="idUtilisateur" target-entity="Utilisateur" fetch="LAZY">
      <join-columns>
        <join-column name="id_utilisateur" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idCategorie" target-entity="Categorie" fetch="LAZY">
      <join-columns>
        <join-column name="id_categorie" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
