<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Finance" table="finance">
    <indexes>
      <index name="fk3" columns="id_facture"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="taxe" type="float" column="taxe" precision="10" scale="0" nullable="false"/>
    <field name="tva" type="integer" column="tva" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="photo" type="string" column="photo" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prix" type="float" column="prix" precision="10" scale="0" nullable="false"/>
    <field name="etat" type="string" column="etat" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="date" type="date" column="date" nullable="false"/>
    <many-to-one field="idFacture" target-entity="Facture" fetch="LAZY">
      <join-columns>
        <join-column name="id_facture" referenced-column-name="id_facture"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
