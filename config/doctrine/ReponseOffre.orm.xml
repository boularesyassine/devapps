<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\ReponseOffre" table="reponse_offre">
    <indexes>
      <index name="IDX_406FFD0C4103C75F" columns="id_offre"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomProduit" type="string" column="nom_produit" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="budget" type="float" column="budget" precision="10" scale="0" nullable="false"/>
    <field name="etat" type="string" column="Etat" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="date" type="date" column="date" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <many-to-one field="idOffre" target-entity="AppelD'offre" fetch="LAZY">
      <join-columns>
        <join-column name="id_offre" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
